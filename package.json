{
  "name": "zommable_network",
  "version": "0.1.0",
  "description": "A template for use on flourish.studio",
  "main": "template.js",
  "author": "wykhuh",
  "license": "ISC",
  "keywords": [
    "flourish"
  ],
  "scripts": {
